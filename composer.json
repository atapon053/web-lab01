{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "Belphemur/laravel-gettext": "6.x",
        "backpack/crud": "^3.4",
        "backpack/langfilemanager": "^1.0",
        "backpack/permissionmanager": "^2.1",
        "barryvdh/laravel-elfinder": "^0.3.12",
        "cviebrock/eloquent-sluggable": "^4.5",
        "dimsav/laravel-translatable": "^9.0",
        "fideloper/proxy": "^4.0",
        "folklore/graphql": "^1.1",
        "google/cloud-translate": "^1.2",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-translatable": "^2.2",
        "yajra/laravel-datatables-oracle": "^8.7"
    },
    "require-dev": {
        "backpack/generators": "^1.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "app/Libraries/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
